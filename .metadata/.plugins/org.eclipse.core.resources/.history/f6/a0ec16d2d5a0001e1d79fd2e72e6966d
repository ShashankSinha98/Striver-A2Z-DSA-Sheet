package codingninjas;

public class FindTheNumberThatAppearsOnceAndTheOtherNumbersTwice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int getSingleElement(int[] arr){
        int res = 0;
        
        for(int i: arr) {
        	res ^= i;
        }
        
        return res;
    }

}
